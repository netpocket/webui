<span>id: <%= @id %></span>
<span>name: unnamed</span>
<ul class="features">
  <% for name, actions of @features: %>
    <li class="feature">
      <span><%= name %></span>
      <ul class="actions">
        <% for action, dummy of actions: %>
          <li>
            <a href="#" data-name="<%= name %>:<%= action %>">
              <%= action %>
            </a>
          </li>
        <% end %>
      </ul>
    </li>
  <% end %>
</ul>
